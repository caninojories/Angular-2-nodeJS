{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run global && npm install && npm run run",
    "run": "nodemon back-end/server.js",
    "run-prod": "NODE_ENV=production nodemon back-end/server.js",
    "typescript-check": "concurrently \"npm run tsc:w\" \"nodemon back-end/server.js\"",
    "typescript-watch": "concurrently \"nodemon back-end/server.js\" \"gulp watchJS\"",
    "global": "npm install typings -g && npm install concurrently -g",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "license": "ISC",
  "dependencies": {
    "a2-in-memory-web-api": "^0.1.8",
    "angular2": "^2.0.0-beta.8",
    "body-parser": "^1.15.0",
    "chalk": "^1.1.1",
    "compression": "^1.6.1",
    "ejs": "^2.4.1",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "express": "^4.13.4",
    "method-override": "^2.3.5",
    "mongoose": "^4.4.5",
    "mongoose-timestamp": "^0.5.0",
    "morgan": "^1.7.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "^5.0.0-beta.2",
    "serve-favicon": "^2.3.0",
    "systemjs": "0.19.20",
    "typescript": "^1.8.2",
    "yargs": "^4.2.0",
    "zone.js": "^0.5.15"
  },
  "devDependencies": {
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-csslint": "^0.2.2",
    "gulp-inject": "^3.0.0",
    "gulp-less": "^3.0.5",
    "gulp-load-plugins": "^1.2.0",
    "gulp-notify": "^2.2.0",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.5",
    "run-sequence": "^1.1.5",
    "serve-static": "^1.10.2",
    "typescript": "^1.7.5",
    "typings": "^0.6.8",
    "wiredep": "^3.0.0"
  }
}
